<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>

    <handlers>
      <add name="iisnode"
           path="Server.js"
           verb="*"
           modules="iisnode" />
    </handlers>

    <rewrite>
      <rules>
        <rule name="NodeApp" stopProcessing="true">
          <match url=".*" />
          <action type="Rewrite" url="Server.js" />
        </rule>
      </rules>
    </rewrite>

    <directoryBrowse enabled="false" />

  </system.webServer>
</configuration>
